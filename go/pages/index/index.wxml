<!--index.wxml-->
<swiper indicator-dots='true' autoplay='true' interval='5000' duration='100' >
  <block wx:for="{{imgsrc}}" wx:key="id">
    <swiper-item>
      <image src="{{item.url}}"></image>
    </swiper-item>
  </block>
</swiper>
<view class="setTitle">
  <block wx:for="{{arricon}}" wx:key="unique" for:key="id">
    <navigator class="tab" url="{{item.url}}">
      <image src="{{item.icon}}" class="tab-img"></image>
      <text class="tab-text">{{item.title}}</text>
    </navigator>
  </block>
</view>
<view class="vidox">
  <text>教学资质</text>
  <video src='{{vidosrc}}' class='vido'></video>
</view>